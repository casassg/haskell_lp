INPUT X
INPUT Y
IF X > 0 OR X = 0 OR NOT 0 > Y THEN
	Z := 1
	WHILE X > Y
	DO
		X := X - 1
		Z := Z +1
	END
ELSE
	Z := 0
END
PRINT Z

INPUT X

WHILE X > 0 OR X = 0
DO
	INPUT Y
	PUSH P Y
END
S := 0
SIZE P L
WHILE L > 0
DO
	POP P Y
	S := S + Y
	L := L - 1
END
PRINT S


INPUT X
INPUT Y
IF X > 0 OR X = 0 OR NOT 0 > Y THEN
	Z := 1
	WHILE X > Y
	DO
		X := X - 1
		Z := Z +1
	END
ELSE
	Z := 0
END

EMPTY P
SIZE P L
PRINT L
S := L+3+4
